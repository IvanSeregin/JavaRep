package application.view;

public enum ButtonAction {
	 
    b1 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b1");
                    return 1;
                }
            ;
    }, 
   b2 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b2");
                    return 2;
                }
            ;
    },
    b3 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b3");
                    return 3;
                }
            ;
    }, 
    b4 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b4");
                    return 4;
                }
            ;
    }, 
    b5 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b5");
                    return 5;
                }
            ;
 
    }; 
   public abstract int doAction();
}