package application.view;

public enum ButtonAction {
	 
    b1 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b1");
                    return 1;
                }
            ;
    }, 
   b2 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b2");
                    return 2;
                }
            ;
    },
    b3 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b3");
                    return 3;
                }
            ;
    }, 
    b4 {
                @Override
                public int doAction() {
                    System.out.println("You clicked me b4");
                    return 4;
                }
            ;
    }, 
    b5 
    {
	    @Override
	    public int doAction() 
	    {
	        return 5;
	    }; 
    },
    b6 
    {
	    @Override
	    public int doAction() 
	    {
	        return 6;
	    }; 
    },
    b7 
    {
	    @Override
	    public int doAction() 
	    {
	        return 8;
	    }; 
    },
    b8 
    {
	    @Override
	    public int doAction() 
	    {
	        return 8;
	    }; 
    },
    b9 
    {
	    @Override
	    public int doAction() 
	    {
	        return 9;
	    }; 
    },
    b0 
    {
	    @Override
	    public int doAction() 
	    {
	        return 0;
	    }; 
    }; 
   public abstract int doAction();
}